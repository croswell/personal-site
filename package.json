{
  "name": "samcroswell-personal-site",
  "version": "1.0.0",
  "description": "Personal portfolio website for Sam Croswell - Founding Product Designer",
  "main": "index.js",
  "scripts": {
    "build": "tailwindcss -i ./input.css -o ./css/style.css --minify",
    "watch": "tailwindcss -i ./input.css -o ./css/style.css --watch",
    "convert-webp": "node scripts/convert-webp.js",
    "optimize-video": "node scripts/optimize-video.js",
    "build-prod": "npm run convert-webp && npm run build"
  },
  "keywords": [
    "portfolio",
    "design",
    "product-design",
    "personal-website"
  ],
  "author": "Sam Croswell",
  "license": "ISC",
  "devDependencies": {
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0"
  }
}
